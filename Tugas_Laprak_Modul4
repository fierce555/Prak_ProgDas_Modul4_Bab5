#include <iostream>
#include <vector>
#include <string>
using namespace std;

bool isPrima(int bilangan) {
    if (bilangan <= 1) {
        return false;
    }
    for (int i = 2; i * i <= bilangan; i++) {
        if (bilangan % i == 0) {
            return false;
        }
    }
    return true;
}
